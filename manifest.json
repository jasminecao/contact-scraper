{
	"name": "Anchor",
	"version": "1.0",
	"description": "Anchor Chrome Extension",
	"permissions": [
		"activeTab",
		"declarativeContent",
		"storage",
		"tabs",
		"<all_urls>"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
    "default_icon": "Anchor.png",
		"default_popup": "popup.html"
  },
  "icons": {
    "16": "Anchor.png",
    "48": "Anchor.png",
    "128": "Anchor.png"
  },
	"manifest_version": 2
}
